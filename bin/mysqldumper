#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

use League\CLImate\CLImate;
use dployer\Command;
use Symfony\Component\Console\Application;

$application = new Application('mysqldumper', '0.1-dev');
$application->add(new Command\mysqldumper(new Git, new CLImate));
$application->run();
